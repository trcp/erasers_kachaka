<?xml version="1.0"?>
<launch>
  <node name="robot_state_publisher" pkg="robot_state_publisher" exec="robot_state_publisher" output="screen">
    <param name="robot_description" type="str" value="$(command '$(find-pkg-prefix xacro)/bin/xacro $(find-pkg-share erasers_kachaka_description)/urdf/kachaka2shelf.urdf.xacro')" />
    <remap from="/joint_states" to="/er_kachaka/base/joint_states" />
    <remap from="/robot_description" to="/er_kachaka/base/robot_description" />
  </node>
</launch>